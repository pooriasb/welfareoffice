@model BehzistiMaskan.ViewModels.ClientWaitingListSuccessfulRegisterViewMode
@{
    ViewBag.Title = "ثبت نام موفق مددجو";
    Layout = "~/Views/Shared/_LayoutSimpleWithHeaderBehzisti.cshtml";
}

<div class="inner-wrapper">
    <section role="main" class="container">

        <div class="row">
            <div class="col-md-12">

                <section class="panel panel-featured panel-featured-success">
                    <div class="panel-heading"><h3>ثبت نام موفق</h3></div>
                    <div class="panel-body">
                        <div class="alert alert-success" style="line-height: 2.5">

                            @(Model.IsMale ? "جناب آقای " : "سرکار خانم ") <b> @Model.Name @Model.Family </b>
                            ثبت نام شما در سامانه سبا با موفقیت انجام شد. شما برای ورود به سامانه می بایست از کد ملی خود و کد رهگیری زیر استفاده نمایید.
                            <br />
                            لطفا اطلاعات موجود در این صفحه را یادداشت نموده و یا با موبایل از آن عکس بگیرید
                        </div>


                        <div class="row text-xl">
                            <div class="col-md-3">
                                کد ملی:
                            </div>
                            <div class="col-md-9 text-primary">
                                @Model.NationalCode
                            </div>
                        </div>

                        <div class="row text-xl">
                            <div class="col-md-3">
                                کد رهگیری:
                            </div>
                            <div class="col-md-9 text-success">
                                @Model.ActivationCode
                            </div>
                        </div>
                    </div>
                    <div class="panel-footer">
                        <a href="@Url.Action("Index")" class="btn btn-primary">ورود به سامانه</a>
                    </div>
                </section>

            </div>
        </div>

    </section>
</div>
